// 将设置放入此文件中以覆盖默认值和用户设置。
{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/.DS_Store": true,
        "**/*.map":true,
        "**/*.js": {
            "when": "$(basename).ts"
        }
    },
    "javascript.validate.enable": false,
    "eslint.enable": true
    
}